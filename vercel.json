{
    "version": 2,
    "builds": [{
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
            "buildCommand": "yarn build",
            "includeFiles": ["prisma/schema.prisma"]
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "dist/index.js"
    }]
}